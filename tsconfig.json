{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "strict": true,
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "outDir": "./public/build",
      "noEmit": false
    },
    "include": ["assets/**/*"],
    "exclude": ["node_modules"]
  }